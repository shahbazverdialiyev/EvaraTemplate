@using System.Collections
@model Dictionary<string,Setting>
<form class="col-6" method="post">
    @foreach(KeyValuePair<string,Setting> item in Model)
    {
        <div class="form-group">
            <label asp-for="@item.Value.Value">@item.Key</label>
            <input asp-for="@item.Value.Value" class="form-control">
            <span asp-validation-for="@item.Value.Value"></span>
        </div>
    }
    <button type="submit" class="btn btn-primary">Update</button>
</form>