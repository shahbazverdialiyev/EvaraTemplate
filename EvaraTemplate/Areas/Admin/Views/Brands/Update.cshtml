@using EvaraTemplate.ViewModels.BrandVM;
@model SetBrandVM
<form class="col-6" method="post" enctype="multipart/form-data">
    <div class="form-group">
        <label asp-for="Name"></label>
        <input asp-for="Name" class="form-control">
        <span asp-validation-for="Name"></span>
    </div>
    <div class="form-group">
        <label asp-for="formImage"></label>
        <input asp-for="formImage" class="form-control">
        <span asp-validation-for="formImage"></span>
    </div>
    <button type="submit" class="btn btn-primary">Update</button>
</form>